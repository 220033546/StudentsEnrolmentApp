@page "/"
@using Microsoft.AspNetCore.Authorization
@attribute [Authorize]
@inject IAccountService AccountService

<div class="p-4">
    <div class="container">
        <h1>Hi @AccountService.User.FirstName!</h1>
        <p>You're logged in Student Portal!!</p>
        <p><NavLink href="users">Admin</NavLink></p>
        <p><NavLink href="ManageCourse/AddCourse">Add Course</NavLink></p>
        <p><NavLink href="ManageCourse">View a List of Courses</NavLink></p>
    </div>
</div>